a, b, c = map(int, input("Enter the three sides of the triangle: ").split())

if a == b and b == c:
    print("Equilateral")
if a == b and b != c:
    print("Isosceles")
if a != b and b == c:
    print("Isosceles")
if a != b and b != c and a== c:
    print("Isosceles")
if a != b and b != c and a != c:
    print("Scalene")
